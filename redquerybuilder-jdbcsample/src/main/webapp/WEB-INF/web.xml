<?xml version="1.0" encoding="UTF-8"?>

<web-app xmlns="http://java.sun.com/xml/ns/javaee" 
         version="2.5" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
         
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>search</servlet-name>
		<servlet-class>com.redspr.redquerybuilder.sample.SearchServlet</servlet-class>
		<load-on-startup>1</load-on-startup>
	</servlet>

	<servlet-mapping>
		<servlet-name>search</servlet-name>
		<url-pattern>/search</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>meta</servlet-name>
		<servlet-class>com.redspr.redquerybuilder.sample.MetaServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>meta</servlet-name>
		<url-pattern>/meta</url-pattern>
	</servlet-mapping>
</web-app>
